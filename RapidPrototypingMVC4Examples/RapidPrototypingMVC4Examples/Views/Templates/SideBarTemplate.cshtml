@using Prototyping.Ipsum
@using Prototyping.Placeholdit
<!-- Header and Nav -->

<div class="row">
    <div class="three columns">
        <h1>@Html.Placehold(400, 100, "Logo")</h1>
    </div>
    <div class="nine columns">
        @Html.Ipsum().ul(4, 2, true, new { @class = "link-list right" })
    </div>
</div>

<!-- End Header and Nav -->

<div class="row">

    <!-- Main Content Section -->
    <!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
    <div class="nine columns push-three">

        <h3>Page Title <small>Page subtitle</small></h3>
        @Html.Ipsum().p().p().p().p()
    </div>


    <!-- Nav Sidebar -->
    <!-- This is source ordered to be pulled to the left on larger screens -->
    <div class="three columns pull-nine">
        @Html.Ipsum().ul(6, 2, true, new { @class = "side-nav" })
        <p>
            @Html.Placehold(320, 240, "Ad")
        </p>

    </div>

</div>


<!-- Footer -->

<footer class="row">
    <div class="twelve columns">
        <hr />
        <div class="row">
            <div class="six columns">
                <p>&copy; Copyright no one at all. Go to town.</p>
            </div>
            <div class="six columns">
                @Html.Ipsum().ul(4, 2, true, new { @class = "link-list right" })
            </div>
        </div>
    </div>
</footer>
